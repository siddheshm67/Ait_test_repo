package com.ssn.api.service;

import org.springframework.stereotype.Service;

@Service
public class DataService {
	
	public boolean getSsnState(String ssn) {
		
		String firstDigit = ssn.substring(1);
		String stateName;
		
		switch(firstDigit) {
		  case "1":
			  stateName = "";
		    break;
		  case "2":
			  stateName = "";
		    break;
		  case "3":
			    
			    break;
		  case "4":
			    
			    break;
		  case "5":
			    
			    break;
		  case "6":
			    
			    break;
		  case "7":
			    
			    break;
		  case "8":
			    
			    break;
		  case "9":
			    
			    break;
			    
		  default:
			 stateName = "";
		}
		
		return true;
	}

}
